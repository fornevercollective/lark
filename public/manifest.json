{
  "name": "Lark IANA - AI Development Environment",
  "short_name": "Lark",
  "description": "A unified AI development environment for modern creators with terminal-centric design and comprehensive toolset integration.",
  "version": "1.0.0",
  "start_url": "/",
  "display": "standalone",
  "orientation": "any",
  "theme_color": "#030213",
  "background_color": "#ffffff",
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
  "categories": ["development", "productivity", "utilities"],
  "shortcuts": [
    {
      "name": "Terminal",
      "short_name": "Terminal",
      "description": "Open development terminal",
      "url": "/?mode=terminal",
      "icons": [
        {
          "src": "/icons/terminal-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "AI Chat",
      "short_name": "AI",
      "description": "Start AI conversation",
      "url": "/?mode=ai",
      "icons": [
        {
          "src": "/icons/ai-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Editor",
      "short_name": "Editor",
      "description": "Open markdown editor",
      "url": "/?mode=editor",
      "icons": [
        {
          "src": "/icons/editor-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Codex",
      "short_name": "Codex",
      "description": "Knowledge manager",
      "url": "/?mode=codex",
      "icons": [
        {
          "src": "/icons/codex-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "icons": [
    {
      "src": "/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "density": "1.5"
    },
    {
      "src": "/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "density": "2.0"
    },
    {
      "src": "/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "density": "2.6"
    },
    {
      "src": "/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "density": "3.0"
    },
    {
      "src": "/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "density": "3.2"
    },
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "density": "4.0"
    },
    {
      "src": "/icons/icon-384x384.png",
      "sizes": "384x384",
