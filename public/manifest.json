    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "density": "10.7"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/desktop-wide.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Lark desktop interface showing terminal and editor"
    },
    {
      "src": "/screenshots/desktop-narrow.png",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Lark mobile interface with sidebar collapsed"
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://lark.figma.site"
    },
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.lark.iana",
      "id": "com.lark.iana"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/lark-iana/id123456789",
      "id": "123456789"
    },
    {
      "platform": "windows",
      "url": "https://www.microsoft.com/store/apps/lark-iana"
    }
  ],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "lark",
      "url": "/?handler=protocol&url=%s"
    },
    {
      "protocol": "web+lark",
      "url": "/?handler=web&url=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/?handler=file",
      "accept": {
        "text/markdown": [".md", ".markdown"],
        "text/plain": [".txt"],
        "application/json": [".json"],
        "text/yaml": [".yml", ".yaml"],
        "application/javascript": [".js"],
        "text/typescript": [".ts"],
        "text/python": [".py"]
      }
    }
  ],
  "share_target": {
    "action": "/?handler=share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["text/*", "application/json", ".md", ".txt", ".py", ".js", ".ts"]
        }
      ]
    }
  },
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
}